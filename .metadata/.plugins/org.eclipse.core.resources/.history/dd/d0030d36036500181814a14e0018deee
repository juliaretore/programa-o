package br.transbus.classes;

public class Reserva {
	private String viagem;
	
	public Reserva(String viagem) {
	this.viagem = viagem;
	}

	public String getviagem() {
		return viagem;
	}

	public void setviagem(String viagem) {
		this.viagem = viagem;
	}


	public void reservaOnibus(String nome) {
	System.out.println("Viagem agendada para: " +viagem+ " para "+nome);
	}

}
