package br.transbus.classes;

public class Viagem {
	private String viagem;
	
	public Viagem(String viagem) {
	this.viagem = viagem;
	}

	public String getviagem() {
		return viagem;
	}

	public void setviagem(String viagem) {
		this.viagem = viagem;
	}


	public void reservaOnibus(String nome) {
	System.out.println("Viagem com visitas aos pontos turisticos agendada para: " +viagem+ " para "+nome);
	}

}
